<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>projet_fil_rouge.WEB</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<include-prelude>/WEB-INF/taglib.jsp</include-prelude>
		</jsp-property-group>
	</jsp-config>
	<servlet>
		<servlet-name>Connexion</servlet-name>
		<servlet-class>servlets.Connexion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Connexion</servlet-name>
		<url-pattern>/connexion</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CreerCivilite</servlet-name>
		<servlet-class>servlets.ServletCivilite</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreerCivilite</servlet-name>
		<url-pattern>/creerCivilite</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CreerAdresse</servlet-name>
		<servlet-class>servlets.ServletAdresse</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreerAdresse</servlet-name>
		<url-pattern>/creerAdresse</url-pattern>
	</servlet-mapping>
</web-app>